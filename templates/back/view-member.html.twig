{% extends 'base-back.html.twig' %}

{% block body %}
<div class="col-md-12">
        <div class="card card-plain">
          <div class="card-header card-header-primary">
            <h4 class="card-title mt-0"> Publications</h4>
            <p class="card-category"></p>
          </div>
          <div class="card-body">
            <div class="table-responsive">
              <table class="table table-hover">
                <thead>
                  <tr>
                    <th class="none">             
                    </th>
                    <th>Nom</th>
                    <th>Pseudo</th>
                    <th>Email</th>
                    <th>Date d'inscription</th>
                  </tr>
                </thead>
                <tbody>
                  <!-- Liste des posts -->
                  {% for member in members %}
                  <tr>
                    <td class="none"></td>
                    <td>{{ member.nom }}</td>
                    <td>{{ member.pseudo }}</td>
                    <td>{{ member.mail }}</td>  
                    <td>{{ member.createdAt | date ('d/m/Y') }}</td> 
                  </tr>  
                {% endfor %} 
                </tbody>
              </table>
            </div>
          </div>
        </div>
        {{ knp_pagination_render(members) }}
{% endblock %}